// Freight Forwarding - Shared Types
type ShipmentId is Id(ShipmentContext.Shipment)  with {
  briefly "Shipment identifier"
  described as {
    |Unique identifier for a shipment.
  }
}
type BookingId is UUID with {
  briefly "Booking identifier"
  described as {
    |Reference to a carrier booking.
  }
}
type QuoteId is UUID with {
  briefly "Quote identifier"
  described as {
    |Reference to a rate quote.
  }
}
type PartyId is UUID with {
  briefly "Party identifier"
  described as {
    |Reference to a business party.
  }
}
type ShipmentStatus is any of {
  Quoted,
  Booked,
  PickupScheduledStatus,
  InTransit,
  AtOriginPort,
  OnVessel,
  AtDestinationPort,
  InCustoms,
  OutForDelivery,
  Delivered,
  Cancelled
} with {
  briefly "Shipment status"
  described as {
    |Current shipment state.
  }
}
type ServiceType is any of {
  FCL,
  LCL,
  AirFreight,
  Express,
  RoRo,
  BreakBulk,
  ProjectCargo,
  Reefer,
  DG
} with {
  briefly "Service type"
  described as {
    |Type of freight service.
  }
}
type Incoterms is any of {
  EXW,
  FCA,
  FAS,
  FOB,
  CFR,
  CIF,
  CPT,
  CIP,
  DAP,
  DPU,
  DDP
} with {
  briefly "Incoterms"
  described as {
    |Trade terms (Incoterms 2020).
  }
}
type ContainerType is any of {
  DRY20,
  DRY40,
  DRY40HC,
  REEFER20,
  REEFER40,
  OPENTOP20,
  OPENTOP40,
  FLATRACK20,
  FLATRACK40,
  TANK20
} with {
  briefly "Container type"
  described as {
    |Type of shipping container.
  }
}
type PartyRole is any of {
  ShipperRole,
  ConsigneeRole,
  NotifyParty,
  Forwarder,
  Carrier,
  CustomsBroker,
  Trucker
} with {
  briefly "Party role"
  described as {
    |Role in shipment.
  }
}
type Address is {
  companyName: String(1,200) with {
    briefly "Company"
    described as {
      |Company name.
    }
  }
  street: String(1,200) with {
    briefly "Street"
    described as {
      |Street address.
    }
  }
  city: String(1,100) with {
    briefly "City"
    described as {
      |City.
    }
  }
  state: String(2,50)? with {
    briefly "State"
    described as {
      |State or province.
    }
  }
  postalCode: String(3,20) with {
    briefly "Postal"
    described as {
      |Postal code.
    }
  }
  country: String(2,3) with {
    briefly "Country"
    described as {
      |Country code.
    }
  }
  contactName: String(1,200) with {
    briefly "Contact"
    described as {
      |Contact name.
    }
  }
  contactPhone: String(10,20) with {
    briefly "Phone"
    described as {
      |Contact phone.
    }
  }
  contactEmail: String(5,254) with {
    briefly "Email"
    described as {
      |Contact email.
    }
  }
} with {
  briefly "Address"
  described as {
    |Business address with contact.
  }
}
type ShipmentParty is {
  partyId: PartyId with {
    briefly "Party"
    described as {
      |Party identifier.
    }
  }
  role: PartyRole with {
    briefly "Role"
    described as {
      |Party role.
    }
  }
  address: Address with {
    briefly "Address"
    described as {
      |Party address.
    }
  }
} with {
  briefly "Shipment party"
  described as {
    |Party involved in shipment.
  }
}
type CargoDetails is {
  description: String(1,1000) with {
    briefly "Description"
    described as {
      |Cargo description.
    }
  }
  hsCode: String(6,10) with {
    briefly "HS code"
    described as {
      |Harmonized system code.
    }
  }
  packages: Natural with {
    briefly "Packages"
    described as {
      |Number of packages.
    }
  }
  packageType: String(1,50) with {
    briefly "Type"
    described as {
      |Package type.
    }
  }
  grossWeight: Decimal(12,3) with {
    briefly "Weight"
    described as {
      |Gross weight (kg).
    }
  }
  volume: Decimal(10,3) with {
    briefly "Volume"
    described as {
      |Volume (CBM).
    }
  }
  marksAndNumbers: String(1,500)? with {
    briefly "Marks"
    described as {
      |Marks and numbers.
    }
  }
  hazardousGoods: Boolean with {
    briefly "DG"
    described as {
      |Is dangerous goods.
    }
  }
  unNumber: String(4,4)? with {
    briefly "UN"
    described as {
      |UN number if DG.
    }
  }
  temperatureControl: TemperatureRange? with {
    briefly "Temp"
    described as {
      |Temperature requirements.
    }
  }
} with {
  briefly "Cargo details"
  described as {
    |Cargo description and dimensions.
  }
}
type TemperatureRange is {
  minTemp: Decimal(5,2) with {
    briefly "Min"
    described as {
      |Minimum temperature (C).
    }
  }
  maxTemp: Decimal(5,2) with {
    briefly "Max"
    described as {
      |Maximum temperature (C).
    }
  }
} with {
  briefly "Temperature range"
  described as {
    |Required temperature range.
  }
}
type ContainerInfo is {
  containerNumber: String(11,11) with {
    briefly "Number"
    described as {
      |Container number.
    }
  }
  containerType: ContainerType with {
    briefly "Type"
    described as {
      |Container type.
    }
  }
  sealNumber: String(1,50) with {
    briefly "Seal"
    described as {
      |Seal number.
    }
  }
  tareWeight: Decimal(8,2) with {
    briefly "Tare"
    described as {
      |Tare weight (kg).
    }
  }
  containerGrossWeight: Decimal(10,2) with {
    briefly "Gross"
    described as {
      |Gross weight (kg).
    }
  }
} with {
  briefly "Container info"
  described as {
    |Container details.
  }
}
type RouteInfo is {
  originPort: String(5,5) with {
    briefly "Origin"
    described as {
      |Origin port code.
    }
  }
  destinationPort: String(5,5) with {
    briefly "Destination"
    described as {
      |Destination port code.
    }
  }
  transshipmentPorts: String(5,5)* with {
    briefly "Transshipment"
    described as {
      |Transshipment ports.
    }
  }
  estimatedTransitDays: Natural with {
    briefly "Transit"
    described as {
      |Estimated transit days.
    }
  }
} with {
  briefly "Route info"
  described as {
    |Shipping route details.
  }
}
type CarrierBooking is {
  bookingId: BookingId with {
    briefly "Booking"
    described as {
      |Booking reference.
    }
  }
  carrierName: String(1,200) with {
    briefly "Carrier"
    described as {
      |Carrier name.
    }
  }
  bookingNumber: String(1,50) with {
    briefly "Number"
    described as {
      |Carrier booking number.
    }
  }
  bookedVesselName: String(1,200)? with {
    briefly "Vessel"
    described as {
      |Vessel name.
    }
  }
  bookedVoyageNumber: String(1,50)? with {
    briefly "Voyage"
    described as {
      |Voyage number.
    }
  }
  bookedFlightNumber: String(1,20)? with {
    briefly "Flight"
    described as {
      |Flight number.
    }
  }
  etd: TimeStamp with {
    briefly "ETD"
    described as {
      |Estimated departure.
    }
  }
  eta: TimeStamp with {
    briefly "ETA"
    described as {
      |Estimated arrival.
    }
  }
} with {
  briefly "Carrier booking"
  described as {
    |Carrier booking details.
  }
}
type RateQuote is {
  quoteId: QuoteId with {
    briefly "Quote"
    described as {
      |Quote identifier.
    }
  }
  validFrom: Date with {
    briefly "From"
    described as {
      |Quote valid from.
    }
  }
  validTo: Date with {
    briefly "To"
    described as {
      |Quote valid to.
    }
  }
  oceanFreight: Decimal(10,2) with {
    briefly "Ocean"
    described as {
      |Ocean freight charge.
    }
  }
  originCharges: Decimal(10,2) with {
    briefly "Origin"
    described as {
      |Origin charges.
    }
  }
  destinationCharges: Decimal(10,2) with {
    briefly "Destination"
    described as {
      |Destination charges.
    }
  }
  documentationFee: Decimal(8,2) with {
    briefly "Doc fee"
    described as {
      |Documentation fee.
    }
  }
  totalAmount: Decimal(12,2) with {
    briefly "Total"
    described as {
      |Total quoted amount.
    }
  }
  currency: String(3,3) with {
    briefly "Currency"
    described as {
      |Quote currency.
    }
  }
} with {
  briefly "Rate quote"
  described as {
    |Shipping rate quote.
  }
}
type MilestoneEvent is {
  eventType: String(1,100) with {
    briefly "Event"
    described as {
      |Milestone type.
    }
  }
  location: String(1,200) with {
    briefly "Location"
    described as {
      |Event location.
    }
  }
  occurredAt: TimeStamp with {
    briefly "Occurred"
    described as {
      |Event time.
    }
  }
  description: String(1,500) with {
    briefly "Description"
    described as {
      |Event description.
    }
  }
} with {
  briefly "Milestone event"
  described as {
    |Shipment tracking milestone.
  }
}
type DocumentInfo is {
  documentType: String(1,100) with {
    briefly "Type"
    described as {
      |Document type.
    }
  }
  documentNumber: String(1,100) with {
    briefly "Number"
    described as {
      |Document number.
    }
  }
  issuedDate: Date with {
    briefly "Issued"
    described as {
      |Issue date.
    }
  }
  fileReference: String(1,500) with {
    briefly "File"
    described as {
      |Document file reference.
    }
  }
} with {
  briefly "Document info"
  described as {
    |Shipping document details.
  }
}
