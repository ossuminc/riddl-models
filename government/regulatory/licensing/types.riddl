    described as {
      |City.
    }
  }
  addressState: String(2,50) with {
    briefly "State"
    described as {
      |State.
    }
  }
  postalCode: String(5,10) with {
    briefly "Postal"
    described as {
      |ZIP code.
    }
  }
} with {
  briefly "Address"
  described as {
    |Physical address.
  }
}
