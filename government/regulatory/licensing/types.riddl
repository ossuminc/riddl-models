type LicenseApplication is {
  applicationId: ApplicationId with {
    briefly "Application"
    described as {
      |Application ID.
    }
  }
  licenseType: LicenseType with {
    briefly "Type"
    described as {
      |License type.
    }
  }
  profession: String(1,200) with {
    briefly "Profession"
    described as {
      |Licensed profession.
    }
  }
  applicant: ApplicantInfo with {
    briefly "Applicant"
    described as {
      |Applicant details.
    }
  }
  education: EducationRecord+ with {
    briefly "Education"
    described as {
      |Education history.
    }
  }
  experience: ExperienceRecord* with {
    briefly "Experience"
    described as {
      |Work experience.
    }
  }
  examScores: ExamScore* with {
    briefly "Exams"
    described as {
      |Exam results.
    }
  }
  submittedAt: TimeStamp with {
    briefly "Submitted"
    described as {
      |Submission time.
    }
  }
} with {
  briefly "License application"
  described as {
    |Application details.
  }
}
