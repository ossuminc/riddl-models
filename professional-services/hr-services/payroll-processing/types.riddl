// Payroll Processing - Shared Types
type PayrollRunId is Id(PayrollContext.PayrollRun)  with {
  briefly "Payroll run identifier"
  described as {
    |Unique identifier for a payroll run.
  }
}
type EmployeeId is UUID with {
  briefly "Employee identifier"
  described as {
    |Reference to an employee.
  }
}
type PayPeriodId is UUID with {
  briefly "Pay period identifier"
  described as {
    |Reference to a pay period.
  }
}
type PayrollStatus is any of {
  Draft,
  Processing,
  PendingApproval,
  Approved,
  Paid,
  Voided
} with {
  briefly "Payroll status"
  described as {
    |Payroll run status.
  }
}
type PayFrequency is any of {
  Weekly,
  BiWeekly,
  SemiMonthly,
  Monthly
} with {
  briefly "Pay frequency"
  described as {
    |Payroll frequency.
  }
}
type PayType is any of {
  Salary,
  Hourly,
  Commission,
  Contractor
} with {
  briefly "Pay type"
  described as {
    |Employee compensation type.
  }
}
type TaxFilingStatus is any of {
  Single,
  MarriedFilingJointly,
  MarriedFilingSeparately,
  HeadOfHousehold
} with {
  briefly "Filing status"
  described as {
    |Tax filing status.
  }
}
type PayPeriod is {
  periodId: PayPeriodId with {
    briefly "Period"
    described as {
      |Period identifier.
    }
  }
  startDate: Date with {
    briefly "Start"
    described as {
      |Period start date.
    }
  }
  endDate: Date with {
    briefly "End"
    described as {
      |Period end date.
    }
  }
  payDate: Date with {
    briefly "Pay date"
    described as {
      |Payment date.
    }
  }
  frequency: PayFrequency with {
    briefly "Frequency"
    described as {
      |Pay frequency.
    }
  }
} with {
  briefly "Pay period"
  described as {
    |Payroll period definition.
  }
}
type EarningsLine is {
  earningsType: String(1,50) with {
    briefly "Type"
    described as {
      |Earnings type.
    }
  }
  hours: Decimal(6,2)? with {
    briefly "Hours"
    described as {
      |Hours worked.
    }
  }
  rate: Decimal(10,4) with {
    briefly "Rate"
    described as {
      |Pay rate.
    }
  }
  earningsAmount: Decimal(12,2) with {
    briefly "Amount"
    described as {
      |Earnings amount.
    }
  }
} with {
  briefly "Earnings line"
  described as {
    |Individual earnings entry.
  }
}
type DeductionLine is {
  deductionType: String(1,50) with {
    briefly "Type"
    described as {
      |Deduction type.
    }
  }
  deductionAmount: Decimal(10,2) with {
    briefly "Amount"
    described as {
      |Deduction amount.
    }
  }
  preTax: Boolean with {
    briefly "Pre-tax"
    described as {
      |Is pre-tax deduction.
    }
  }
} with {
  briefly "Deduction line"
  described as {
    |Individual deduction entry.
  }
}
type TaxWithholding is {
  taxType: String(1,50) with {
    briefly "Type"
    described as {
      |Tax type.
    }
  }
  jurisdiction: String(1,50) with {
    briefly "Jurisdiction"
    described as {
      |Tax jurisdiction.
    }
  }
  taxableWages: Decimal(12,2) with {
    briefly "Taxable"
    described as {
      |Taxable wages.
    }
  }
  taxAmount: Decimal(10,2) with {
    briefly "Amount"
    described as {
      |Tax amount.
    }
  }
} with {
  briefly "Tax withholding"
  described as {
    |Tax withholding entry.
  }
}
type EmployeePayStub is {
  employeeId: EmployeeId with {
    briefly "Employee"
    described as {
      |Employee ID.
    }
  }
  payPeriod: PayPeriod with {
    briefly "Period"
    described as {
      |Pay period.
    }
  }
  earnings: EarningsLine+ with {
    briefly "Earnings"
    described as {
      |Earnings lines.
    }
  }
  deductions: DeductionLine+ with {
    briefly "Deductions"
    described as {
      |Deduction lines.
    }
  }
  taxes: TaxWithholding+ with {
    briefly "Taxes"
    described as {
      |Tax withholdings.
    }
  }
  grossPay: Decimal(12,2) with {
    briefly "Gross"
    described as {
      |Gross pay.
    }
  }
  netPay: Decimal(12,2) with {
    briefly "Net"
    described as {
      |Net pay.
    }
  }
} with {
  briefly "Employee pay stub"
  described as {
    |Employee payroll detail.
  }
}
type PayrollTotals is {
  totalGross: Decimal(15,2) with {
    briefly "Total gross"
    described as {
      |Total gross pay.
    }
  }
  totalDeductions: Decimal(15,2) with {
    briefly "Total deductions"
    described as {
      |Total deductions.
    }
  }
  totalTaxes: Decimal(15,2) with {
    briefly "Total taxes"
    described as {
      |Total tax withholdings.
    }
  }
  totalNet: Decimal(15,2) with {
    briefly "Total net"
    described as {
      |Total net pay.
    }
  }
  employeeCount: Natural with {
    briefly "Employees"
    described as {
      |Employee count.
    }
  }
} with {
  briefly "Payroll totals"
  described as {
    |Payroll run totals.
  }
}
type DirectDeposit is {
  accountType: String(1,20) with {
    briefly "Type"
    described as {
      |Account type.
    }
  }
  routingNumber: String(9,9) with {
    briefly "Routing"
    described as {
      |Routing number.
    }
  }
  accountNumber: String(4,17) with {
    briefly "Account"
    described as {
      |Account number.
    }
  }
  depositAmount: Decimal(12,2) with {
    briefly "Amount"
    described as {
      |Deposit amount.
    }
  }
} with {
  briefly "Direct deposit"
  described as {
    |Direct deposit instruction.
  }
}
